<!DOCTYPE mapper PUBLIC "-//database.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.gyw.camel.BlogMapper">

	<insert id="insertJournal" parameterType="java.util.Map">
		insert into blog
		(name, content) values(#{name}, #{content})
	</insert>

</mapper>
